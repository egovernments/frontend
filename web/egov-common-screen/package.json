{
  "name": "Material-Ui-React",
  "version": "1.10.0",
  "description": "UKD Landing Page",
  "private": true,
  "main": "src/index.js",
  "homepage": "https://ukd-dev.egovernments.org/common/",
  "proxy": "https://ukd-dev.egovernments.org/",
  "dependencies": {
    "@babel/core": "7.14.0",
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "4.11.2",
    "classnames": "2.3.1",
    "moment": "2.29.1",
    "node": "^16.4.0",
    "node-sass": "6.0.0",
    "nouislider": "15.1.0",
    "nvm": "0.0.4",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-datetime": "3.0.4",
    "react-dom": "17.0.2",
    "react-faq-component": "^1.3.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-slick": "0.28.1"
  },
  "devDependencies": {
    "@babel/cli": "7.13.16",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/preset-env": "7.14.1",
    "@babel/preset-react": "7.13.13",    
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9"
  },
  "optionalDependencies": {
    "typescript": "4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && gulp licenses",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "build-package-css": "node-sass src/assets/scss/material-ui-react.scss dist/material-ui-react.css",
    "build-package": "npm run build-package-css && babel src --out-dir dist",
    "compile-sass": "node-sass src/assets/scss/material-ui-react.scss src/assets/css/material-ui-react.css",
    "minify-sass": "node-sass src/assets/scss/material-ui-react.scss src/assets/css/material-ui-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/material-ui-react.scss src/assets/css/material-ui-react.css --source-map true"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": []
  }
}
